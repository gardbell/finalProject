package com.ace.schedule.model;

import java.sql.Date;

public class ScheduleDTO {
	
	private int idx;
	private int com_idx;
	private String sort;
	private String title;
	private String content;
	private Date sdate;
	private int stime;
	private int etime;
	private int smna;
	private int emna;
	
	public ScheduleDTO() {
		// TODO Auto-generated constructor stub
	}

	public ScheduleDTO(int idx, int com_idx, String sort, String title, String content, Date sdate, int stime,
			int etime, int smna, int emna) {
		super();
		this.idx = idx;
		this.com_idx = com_idx;
		this.sort = sort;
		this.title = title;
		this.content = content;
		this.sdate = sdate;
		this.stime = stime;
		this.etime = etime;
		this.smna = smna;
		this.emna = emna;
	}


}
